system:
    default:
        yui:
            modules:
                ez-rawcontentview:
                    requires:
                        - '{{ yui_fieldtype_namespace_identifier }}-{{ fieldtype_identifier }}-view'
                ez-contenteditformview:
                    requires:
                        - '{{ yui_fieldtype_namespace_identifier }}-{{ fieldtype_identifier }}-editview'
                {{ yui_fieldtype_namespace_identifier }}-{{ fieldtype_identifier }}-view:
                    requires: ['ez-fieldview', '{{ fieldtype_identifier }}view-ez-template']
                    path: "%{{ yui_fieldtype_namespace_identifier }}_{{ fieldtype_identifier }}fieldtype.public_dir%/js/views/fields/{{ yui_fieldtype_namespace_identifier }}-{{ fieldtype_identifier }}-view.js"
                {{ yui_fieldtype_namespace_identifier }}-{{ fieldtype_identifier }}-editview:
                    requires: ['ez-fieldeditview', 'event-valuechange', '{{ fieldtype_identifier }}editview-ez-template']
                    path: "%{{ yui_fieldtype_namespace_identifier }}_{{ fieldtype_identifier }}fieldtype.public_dir%/js/views/fields/{{ yui_fieldtype_namespace_identifier }}-{{ fieldtype_identifier }}-editview.js"
                {{ fieldtype_identifier }}view-ez-template:
                    type: 'template'
                    path: "%{{ yui_fieldtype_namespace_identifier }}_{{ fieldtype_identifier }}fieldtype.public_dir%/templates/fields/view/{{ fieldtype_identifier }}.hbt"
                {{ fieldtype_identifier }}editview-ez-template:
                    type: 'template'
                    path: "%{{ yui_fieldtype_namespace_identifier }}_{{ fieldtype_identifier }}fieldtype.public_dir%/templates/fields/edit/{{ fieldtype_identifier }}.hbt"




