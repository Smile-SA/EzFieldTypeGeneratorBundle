system:
    default:
        yui:
            modules:
                ez-rawcontentview:
                    requires:
                        - 'ez-{{ fieldtype_identifier }}-view'
                ez-contenteditformview:
                    requires:
                        - 'ez-{{ fieldtype_identifier }}-editview'
                ez-{{ fieldtype_identifier }}-view:
                    requires: ['ez-fieldview']
                    path: %ez_{{ fieldtype_identifier }}fieldtype.public_dir%/js/views/fields/ez-{{ fieldtype_identifier }}-view.js
                ez-{{ fieldtype_identifier }}-editview:
                    requires: ['ez-fieldeditview', 'event-valuechange', '{{ fieldtype_identifier }}editview-ez-template']
                    path: %ez_{{ fieldtype_identifier }}fieldtype.public_dir%/js/views/fields/ez-{{ fieldtype_identifier }}-editview.js
                {{ fieldtype_identifier }}editview-ez-template:
                    type: 'template'
                    path: %ez_{{ fieldtype_identifier }}fieldtype.public_dir%/templates/fields/edit/{{ fieldtype_identifier }}.hbt




